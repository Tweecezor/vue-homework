@import "../../../styles/mixins.pcss";
.content__main {
  &--list {
    display: flex;
    row-gap: 24px;
    flex-wrap: wrap;
    justify-content: space-between;
  }
  &--item {
    width: 49%;
    @include phones {
      width: 100%;
    }
  }
  &--card {
    min-height: 390px;
  }
  &--block {
    height: 100%;
  }
  &--skills {
    min-height: 84%;
  }
}
